{
  "compilerOptions": {
    "lib": [
      "DOM"
    ],
    "skipLibCheck": true,
    "jsx": "react",
    "target": "esnext",
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    // "verbatimModuleSyntax": true,
    "importsNotUsedAsValues": "error",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true
  }
}
